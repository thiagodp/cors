    "scripts": {
        "test": "kahlan",
        "test2": "kahlan --reporter=verbose",
        "cov": "kahlan --coverage",
        "check": "phpstan analyze src"
    },
    "require": {
        "ext-mbstring": "*"
    },
    "require-dev": {
        "kahlan/kahlan": "^5.2",
